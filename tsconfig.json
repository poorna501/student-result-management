{
  "compilerOptions": {
  "target": "ES6",
  "module": "ESNext",
  "jsx": "react-jsx",
  "declaration": true,
  "emitDeclarationOnly":true,
  "allowImportingTsExtensions": true,
  "baseUrl": "src",
  "paths": {
    "@components/*": ["components/*"],
    "@interface/*": ["interface/*"],
  },
  "moduleResolution": "node",
  "esModuleInterop": true,
},
  "include": ["src/**/*"]
}
